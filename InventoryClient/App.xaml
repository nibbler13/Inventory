<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:InventoryClient"
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="InventoryClient.App"
             ShutdownMode="OnExplicitShutdown">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="NotifyIconResources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <System:String x:Key="String1">Проверьте соответствие израсходованных во время лечения материалов, 
                при необходимости внесите корректировки в список и подтвердите списание</System:String>
            <BitmapImage x:Key="Add" >Add_16x.png</BitmapImage>
            <BitmapImage x:Key="Remove" >Remove_16x.png</BitmapImage>
            <BitmapImage x:Key="Key" >Key_48x.png</BitmapImage>
        </ResourceDictionary>
    </Application.Resources>
</Application>
